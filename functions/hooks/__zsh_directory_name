builtin eval $SETUP
local -a match mbegin mend

case $1 {
  (d)
    case $2 {
      (*) return 1;;
    };;
  (n)
    case $2 {
      (realpath:*)
        typeset -ga reply=( ${${2#*:}:P} );;
      (*) return 1;;
    };;
  (c)
    local expl;
    case $PREFIX {
      (realpath:*)
        compset -P1 '*:'
        _path_files -/ -g'*(-N/)'
        _path_files -g'*(-N^/)' -S']';;
      (*)
        local prefix=( realpath )
        _wanted prefix expl 'prefix' compadd -S':' -a prefix;;
    }
}
